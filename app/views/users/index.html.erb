<div class="col-md-12">
  <table class="table table-hover">
    <thead>
      <tr>
        <td>#</td>
        <td>Id</td>
        <td>Name</td>
        <td>Email</td>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td>#</td>
          <td> <%= user.id %> </td>
          <td> <%= user.name %> </td>
          <td> <%= user.email %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>